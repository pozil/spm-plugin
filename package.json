{
    "name": "spm-plugin",
    "description": "A Salesforce CLI plugin for interacting with the Salesforce Package Manager (SPM) registry",
    "version": "1.0.6",
    "author": "pozil",
    "dependencies": {
        "@oclif/command": "^1.8.16",
        "@oclif/config": "^1.18.3",
        "@oclif/errors": "^1.3.5",
        "@salesforce/command": "^5.2.1",
        "@salesforce/core": "^2.37.1",
        "tslib": "2.4.0"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.26.10",
        "@oclif/plugin-help": "^5.1.12",
        "@oclif/test": "^2.1.0",
        "@salesforce/dev-config": "^3.1.0",
        "@types/jsforce": "^1.9.43",
        "@typescript-eslint/eslint-plugin": "^5.32.0",
        "@typescript-eslint/parser": "^5.32.0",
        "eslint": "^8.21.0",
        "prettier": "^2.7.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "files": [
        "/lib",
        "/messages",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/pozil/spm-plugin",
    "keywords": [
        "sfdx-plugin"
    ],
    "license": "CC0-1.0",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "sfdx",
        "topics": {
            "spm": {
                "description": "Commands to interact with the Salesforce Package Manager (SPM) registry."
            }
        },
        "devPlugins": [
            "@oclif/plugin-help"
        ]
    },
    "bugs": "https://github.com/pozil/spm-plugin/issues",
    "repository": "github:pozil/spm-plugin",
    "scripts": {
        "lint": "eslint . --ext .ts",
        "prettier": "prettier --write '**/*.{js,json,md,ts,yaml,yml}'",
        "prettier:verify": "prettier --list-different '**/*.{js,json,md,ts,yaml,yml}'",
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "tslint -p test -t stylish",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "version": "oclif-dev readme && git add README.md"
    },
    "volta": {
        "node": "14.17.3",
        "yarn": "1.22.15"
    }
}
